package step01_syntax;

public class Ex02Variable {

	public static void main(String[] args) {
		// 타입 변수명 = 값;
		// 기본타입 (primitive) 타입
		// 정수 : -2^(n-1) ~ 2^(n-1)-1
			// n : 비트 수
		
		byte vb1 = -128;
//		byte vb2 = 128; // !ERROR
		short vs1 = 128;
		
		int vi1 = 10;
		int vi2 = 0B1010;
		int vi3 = 012;
		int vi4 = 0XA;
		System.out.println(vi2);
	}

}
